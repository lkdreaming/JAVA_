<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.gyl.domain.base.Repository">
		<id name="rid" length="5">
			<generator class="increment"></generator>
		</id>
		<property name="name" length="20"></property>
		<property name="manager" length="20"></property>
		<set name="products" table="repository_product">
			<key>
				<column name="rid"></column>
			</key>
			<many-to-many class="cn.itcast.gyl.domain.base.Product" column="pid"></many-to-many>
		</set>
	</class>
</hibernate-mapping>
