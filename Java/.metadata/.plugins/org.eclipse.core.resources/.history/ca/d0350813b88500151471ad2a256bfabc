import java.awt.*;
import java.awt.event.*;


public class Yard extends Frame {
	public static final int BLOCK_SIZE = 15;
	public static final int ROWS = 30;
	public static final int COLS = 30;
	public static final String TITLE = "Ã∞≥‘…ﬂ”Œœ∑";
	public static void main(String[] args) {
		new Yard().launch();
	}
	private void launch() {
		Frame f = new Frame();
		Panel p = new Panel();
		f.setTitle(TITLE);
		f.setSize(this.BLOCK_SIZE * this.COLS + 50, this.BLOCK_SIZE * this.ROWS + 50);
		f.setBackground(Color.BLACK);
		f.setVisible(true);
		f.addWindowListener(new WindowAdapter(WindowEvent e));
	}
}
